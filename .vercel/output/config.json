{
  "version": 3,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/api(?:/(.*))$",
      "dest": "/api/index",
      "check": true
    },
    {
      "src": "^(?:/(.*))$",
      "dest": "/index.html",
      "check": true
    },
    {
      "src": "^/api(/.*)?$",
      "status": 404
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    },
    {
      "handle": "miss"
    },
    {
      "src": "^/api/(.+)(?:\\.(?:ts))$",
      "dest": "/api/$1",
      "check": true
    }
  ],
  "framework": {
    "version": "6.3.5"
  },
  "crons": []
}
